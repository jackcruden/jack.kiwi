        "popper.js": "^1.14.7",
        "sass": "^1.17.3",
        "sass-loader": "7.*",
        "tailwindcss": "^0.7.4",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10",
        "webpack-bundle-analyzer": "^3.1.0"
    },
    "dependencies": {
        "glob-all": "^3.1.0",
        "mapbox-gl": "^0.53.1",
